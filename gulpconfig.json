{
    "src": {
        "css": [
            "src/css/**/*.less"
        ],
        "entry": [
            "src/js/**/entry.js"
        ],
        "baseJs": [
            "src/js/base/*.js"
        ],
        "js": [
            "src/js/**/*.js",
            "!src/js/**/entry.js"
        ],
        "html": [
            "src/html/**/*.html"
        ],
        "img": [
            "src/img/**/*.*"
        ],
        "clean": "web/static/**/*.*"
    },
    "dest": {
        "css": "web/static/css/",
        "img": "web/static/img/",
        "js": "web/static/js/",
        "html": "web/static/html/",
        "module": "web/static/module/"
    },
    "res": {
        "css": "web/static/css/**/*.css",
        "js": "web/static/js/**/*.js"
    }
}
